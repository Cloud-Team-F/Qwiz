<div class="login-page">
    <div class="form">
        <input type="text" placeholder="Username" v-model="inputs.username" />
        <input
            type="password"
            placeholder="Password"
            v-model="inputs.password"
        />
        <button @click="login()">
            Login
        </button>
        <button @click="register()">
            Register
        </button>
    </div>
</div>
